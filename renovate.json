{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "gitAuthor": "rtfm-47 <rtfm47@lando.dev>",
  "packageRules": [
    {
      "matchDepNames": [
        "go-supported-list"
      ],
      "matchUpdateTypes": [
        "minor"
      ],
      "enabled": true
    }
  ],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [
        "builders/builders.yml"
      ],
      "matchStrings": [
        "supported:\\n\\s+- '(?<currentValue>.*?)'"
      ],
      "datasourceTemplate": "github-releases",
      "depNameTemplate": "go/go",
      "versioningTemplate": "semver",
      "currentValueTemplate": "go{{currentValue}}"
    }
  ]
}